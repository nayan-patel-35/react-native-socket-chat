  "license": "",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android_clean": "cd android &&./gradlew clean && cd ../",
    "android_build": "cd android && ./gradlew app:assembleRelease",
    "android_production": "ENVFILE=.env react-native run-android",
    "build_android_production": "export ENVFILE=.env && cd android && ./gradlew app:assembleRelease && cd ..",
    "pod_install": "cd ios && arch -x86_64 pod install",
    "clean": "watchman watch-del-all && yarn cache clean && rm -rf ios/build && pod cache clean --all && rm -rf android/app/src/main/res/drawable-* && rm -rf android/build && cd android && ./gradlew clean && cd -"
  },
